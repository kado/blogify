@model BlogifyWebApp.Models.ResultViewModel

@if (Model != null)
{
    if (Model.Message != "")
    {
        <div class="alert alert-dismissible fade show @Model.Type" role="alert">
            <h4 class="alert-heading">@Model.Title</h4>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <p>@Model.Message</p>
        </div>

    }
}
